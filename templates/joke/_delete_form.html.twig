<form method="post" action="{{ path('app_joke_delete', {'id': joke.id}) }}" onsubmit="return confirm('Êtes-vous sûr de bien vouloir supprimer cet élément?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ joke.id) }}">
    <button class="btn"><i class="fa-solid fa-solid fa-trash" style="color: #ff0000;"></i></button>
</form>
