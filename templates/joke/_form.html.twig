

{{ form_start(form) }}

<form data-bitwarden-watching="1">
    <div class="form-group">
      <label class="form-label mt-4">Votre blague</label>
      <input type="text" class="form-control" placeholder="Entrer votre blague" name="{{ field_name(form.text) }}" value="{{ field_value(form.text) }}">
    </div>
    <div class="form-group">
      <label for="exampleSelect1" class="form-label mt-4">Categorie</label>
      <select class="form-select" name="{{ field_name(form.category) }}">

        {% for label, value in field_choices(form.category) %}
            <option value="{{ value }}">{{ label }}</option>
        {% endfor %}
      </select>
    </div>
    {{ form_widget(form) }}

    <button class="btn btn-primary my-4">{{ button_label|default('Ajouter') }}</button>

</form>
{{ form_end(form) }}
